<div class="container-fluid p-0">
  <div class="jumbotron jumbotron-fluid mb-0 p-5" style="background-image: url(./andromeda.png);background-size: cover">
    <div class="container text-light">
      <h1 class="display-4" style="text-shadow: 2px 2px black">Astronomy News Scraper</h1>
      <p class="lead" style="text-shadow: 2px 2px black">The top news associated with celestial objects, space and the physical universe as a whole..</p>
    </div>
  </div>
</div>
  
<div class="container-fluid bg-info m-0 p-0">
  <div class="row">
    <div class="col-md-8" style="display: inline-block;overflow: auto;height: 500px">
      <div class="container" id="articles">
        {{#each articles}}
          {{> article-block }}
        {{/each}}
      </div>
    </div>
    <div class="col-md-4">
      <div id="notes">
      </div>
    </div>
  </div>
</div>